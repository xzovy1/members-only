<div class="member-access mouseable">
    <form action="/join" method="post">
        <div>Who brought you here?</div>
        <label for="secret-answer">Secret Answer:</label>
        <input type="text" name="secret-answer">
        <button type="submit">Submit</button>
    </form>
    <%if(passed == true){%>
        <div>Welcome to the club</div>
    <%}else if(passed == false){%>
        <div>Wrong Answer</div>
    <%}%>
</div>
    <a href="./" class="controls mouseable">Back</a>